@{
    ViewBag.Title = "Check in reservation";
	
}

@section css {
	<link type="text/css"rel="stylesheet" href="/Content/Reservation/checkin.css" />
}

@section javascript {
	<script src="/Scripts/Reservation/Checkin.js" type="text/javascript"></script>
}

@model  CheckInReservationViewModel

<form action="/Reservation/Checkin" class="checkin_form">

<h2>Check in reservation</h2>
<div class="summary">
@Html.Partial("RenderSearchSummary",Model.ReservationSummary)
</div>
<div>
<h3>Select room</h3>
@ViewBag.RenderSection("OccupancyService_NonOccupiedRoomsSelector")
</div>
<div class="fix"></div>
<input type="submit" value="Check In" />

</form>